      <origin xyz="0.162 -0.08192 0.267206" rpy="0 0 0"/>
      <parent link="${prefix}chest"/>
      <child link="${prefix}right_mounting_plate"/>
    </joint>

    <joint name="left_mounting_plate_joint" type="fixed">
      <origin xyz="0.162 0.08192 0.267206" rpy="0 0 0"/>
      <parent link="${prefix}chest"/>
