      <inertial>
        <origin xyz="-0.037004058441535335 -0.00020043772780423342 0.5753345042683139" rpy="0 0 0"/>
        <mass value="33.86884985630993"/>
        <inertia ixx="6.060038" iyy="5.972006" izz="0.308603" ixy="-0.001526" iyz="0.007953" ixz="0.121063"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <!-- <mesh filename="package://stand_chest_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/> -->
          <mesh filename="package://gopher_description/stand_chest_description/meshes/base.stl"/>
